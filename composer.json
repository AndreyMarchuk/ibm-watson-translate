{
    "name": "findbrok/laravel-watson-translate",
    "description": "Use IBM Watson language translation service with laravel",
    "keywords": ["laravel", "ibm", "watson", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "laravel/framework": "~5.0|~5.1|~5.2",
        "findbrok/php-watson-api-bridge": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.0",
        "guzzlehttp/guzzle": "~5.3|~6.0|~6.2"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "FindBrok\\WatsonTranslate\\": "src/",
            "FindBrok\\WatsonTranslate\\Tests\\": "tests/"
        }
    }
}
